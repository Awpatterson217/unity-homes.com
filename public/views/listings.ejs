<!DOCTYPE html>
<html lang="en">
  <% include includes/head.ejs %>
  <body>
    <div class="container">
          
      <% include includes/nav.ejs %>
            
      <div class="row">
        <div class="col-md-12 text-center">
          <form class="container">
            <fieldset class="form-group">
              <legend class="mb-3">
                Filter Listings
              </legend>
              <div class="form-group">
                <div class="row">
                    <label>
                      Rent (USD):
                    </label>
                  <div class="col-md-5 col">
                    <input type="text" class="form-control" name="rentMin" placeholder="Minimum">
                  </div> <!-- END COL -->
                    <label>
                      -
                    </label>
                  <div class="col-md-5 col">
                    <input type="text" class="form-control" name="rentMax" placeholder="Maximum">
                  </div> <!-- END COL -->
                </div> <!-- END ROW -->

                <div class="row">
                  <div class="col-md-4 mt-2">
                    <input type="text" class="form-control" name="city" placeholder="City">
                  </div> <!-- END COL -->

                  <div class="col-md-4 mt-2">
                    <select class="form-control">
                      <option selected disabled>Number of Rooms</option>
                      <option>1</option>
                      <option>2</option>
                      <option>3</option>
                      <option>4</option>
                      <option>5</option>
                    </select>
                  </div> <!-- END COL -->

                  <div class="col-md-4 mt-2">
                    <select class="form-control" name="type">
                      <option selected disabled>Type</option>
                      <option>Single-Family Home</option>
                      <option>Duplex / Double / 2-Family</option>
                      <option>Efficiency / Studio</option>
                      <option>Condominium</option>
                      <option>Apartment</option>
                    </select>
                  </div> <!-- END COL -->
                </div> <!-- END ROW -->
              </div>
              <div class="form-check">
                <label class="form-check-label mr-5">
                  <input type="checkbox" class="form-check-input">
                  Basement
                </label>
                <label class="form-check-label mr-5">
                  <input type="checkbox" class="form-check-input">
                  Garage
                </label>
                <label class="form-check-label mr-5">
                  <input type="checkbox" class="form-check-input">
                  Fence
                </label>
                <label class="form-check-label mr-5">
                  <input type="checkbox" class="form-check-input">
                  Washer / Dryer
                </label>
                <label class="form-check-label mr-5">
                  <input type="checkbox" class="form-check-input">
                  Furnished
                </label>
              </div>
            </fieldset>

            <button type="submit" class="btn btn-md btn-outline-dark">Search</button>
          </form>        
        </div> <!-- END COL -->
      </div> <!-- END ROW -->

      <br />

      <div class="row">
        <div class="col-md-6">
          <div class="card h-100 text-center" style="padding: 10px;">
            <div class="card-body">
              <p class="card-text font-weight-bold">Property Address</p>
              <p class="card-text">Details: Number of rooms, fence, sq ft, etc.</p>
            </div>
          </div>
        </div> <!-- END COL -->

        <div class="col-md-6">
          <div class="card h-100">
            <div class="card-body">
              <div id="carouselOne" class="carousel slide" data-ride="false">
                <ol class="carousel-indicators">
                  <li data-target="#carouselOne" data-slide-to="0" class="active"></li>
                  <li data-target="#carouselOne" data-slide-to="1"></li>
                  <li data-target="#carouselOne" data-slide-to="2"></li>
                </ol>
                <div class="carousel-inner" role="listbox">
                  <div class="carousel-item active">
                    <img class="img-fluid" src="/images/house2.png" alt="Property Image">
                  </div>
                  <div class="carousel-item">
                    <img class="img-fluid" src="/images/house2.png" alt="Property Image">
                  </div>
                  <div class="carousel-item">
                    <img class="img-fluid" src="/images/house2.png" alt="Property Image">
                  </div>
                </div>
                <a class="carousel-control-prev" href="#carouselOne" role="button" data-slide="prev">
                  <span class="carousel-control-prev-icon"></span>
                </a>
                <a class="carousel-control-next" href="#carouselOne" role="button" data-slide="next">
                  <span class="carousel-control-next-icon"></span>
                </a>
              </div>
            </div>
          </div>
        </div> <!-- END COL -->
      </div> <!-- END ROW -->

      <br />

