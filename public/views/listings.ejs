      <div class="row">
        <div class="col-md-12 text-center">
          <form class="container">
            <fieldset class="form-group">
              <legend class="mb-3">Filter Listings</legend>
              <div class="form-group">
                <div class="row">
                    <label>
                      Rent (USD):
                    </label>
                  <div class="col-md-5">
                    <input type="text" class="form-control" name="rentMin" placeholder="Minimum">
                  </div>
                    <label>
                      -
                    </label>
                  <div class="col-md-5">
                    <input type="text" class="form-control" name="rentMax" placeholder="Maximum">
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-4 mt-2">
                    <input type="text" class="form-control" name="city" placeholder="City">
                  </div>
                  <div class="col-md-4 mt-2">
                    <select class="form-control">
                      <option selected disabled>Number of Rooms</option>
                      <option>1</option>
                      <option>2</option>
                      <option>3</option>
                      <option>4</option>
                      <option>5</option>
                    </select>
                  </div>
                  <div class="col-md-4 mt-2">
                    <select class="form-control" name="type">
                      <option selected disabled>Type</option>
                      <option>Single-Family Home</option>
                      <option>Duplex / Double / 2-Family</option>
                      <option>Efficiency / Studio</option>
                      <option>Condominium</option>
                      <option>Apartment</option>
                    </select>
                  </div>
                </div>
              </div>
              <div class="form-check">
                <label class="form-check-label mr-3">
                  <input type="checkbox" class="form-check-input">
                  Basement
                </label>
                <label class="form-check-label mr-3">
                  <input type="checkbox" class="form-check-input">
                  Garage
                </label>
                <label class="form-check-label mr-3">
                  <input type="checkbox" class="form-check-input">
                  Fence
                </label>
                <label class="form-check-label mr-3">
                  <input type="checkbox" class="form-check-input">
                  Washer / Dryer
                </label>
                <label class="form-check-label mr-3">
                  <input type="checkbox" class="form-check-input">
                  Furnished
                </label>
              </div>
            </fieldset>
            <button type="submit" class="btn btn-default">Search</button>
          </form>        
